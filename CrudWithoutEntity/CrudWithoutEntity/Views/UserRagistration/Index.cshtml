@model System.Data.DataTable
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<table class="table table-bordered table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Password</th>
        <th>Is Active</th>
        <th>Action</th>
    </tr>
    @for (int i = 0;i< Model.Rows.Count;i++)
    {
        <tr>
            <td>@Model.Rows[i][1]</td>
            <td>@Model.Rows[i][2]</td>
            <td>@Model.Rows[i][3]</td>
            <td>@Model.Rows[i][4]</td>
            <td>@Model.Rows[i][5]</td>
            <td><a style="margin-right:3px" href="@Url.Action( "Edit","USerRagistration",new{id =@Model.Rows[i][0]})"> Edit</a>|<a style="margin-left:3px" href="@Url.Action( "Delete","USerRagistration",new{id =@Model.Rows[i][0]})">Delete</a></td>
        </tr>
    }

</table>
